{
    "name": "siezi/phile-debugbar",
    "type": "phile-plugin",
    "description": "Add PHP Debug Bar to PhileCMS",
    "license": "MIT",
    "minimum-stability": "dev",
    "keywords": [
        "phile",
        "debugbar"
    ],
    "authors": [
        {
            "name": "Schlaefer",
            "email": "openmail+sourcecode@siezi.com"
        }
    ],
    "require": {
        "phile-cms/phile": "^2.0|dev-develop",
        "middlewares/debugbar": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.2",
        "phpstan/phpstan": "^1.8"
    },
    "config": {
        "allow-plugins": {
            "phile-cms/plugin-installer-plugin": true
        }
    }
}
